<h1>All <%= shoes.length %> Shoes</h1>
<ul>
  <% shoes.forEach(shoe => { %>
  <li>
    <p><%= shoe.name %></p>
    <p>Price: <%= shoe.price %></p>
    <p>Description: <%= shoe.description %></p>
    <span
      >Category: <% shoe.category.forEach(el => { %>
      <p><%= el.name %></p>
      <% }) %>
    </span>
  </li>
  <% }) %>
</ul>
